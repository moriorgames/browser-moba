{% extends 'base.html.twig' %}

{% block body %}
    <div class="page-header">
        <h1>{{ 'map.title_edit'|trans|raw }}</h1>
    </div>

    <div class="row">
        <div class="col-sm-12">

            <div id="map_container">

                {% for tile in tiles %}
                    <a href="#" class="tile {{ tile.class }}"
                       style="top:{{ tile.top }}px;left:{{ tile.left }}px"
                            title="{{ tile.title }}">&nbsp;</a>
                {% endfor %}

            </div>

        </div>
    </div>
{% endblock %}
